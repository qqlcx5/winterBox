
@font-face {
  font-family: iconfont;
  font-weight: normal;
  font-style: normal;
  src: url('https://at.alicdn.com/t/font_906428_w815grhhpbk.ttf') format('truetype');
}
.icon {
  font-size: 48rpx;
  color: #fff;
  font-family: iconfont;
}
.gIcon {
  font-size: 52rpx;
  color: #6c6c6c;
  font-family: iconfont;
}
page {
  font-size: 32rpx;
  background-color: #FFFFFF;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
  flex-direction: column;
  width: 100%;
  min-height: 100vh;
}
.mgr20 {
  margin-right: 20rpx;
}
.mgl20 {
  margin-left: 20rpx;
}
.mgr30 {
  margin-right: 30rpx;
}
.mgl30 {
  margin-left: 30rpx;
}
.mgt100 {
  margin-top: 100rpx;
}
.mgt80 {
  margin-top: 80rpx;
}
.mgt40 {
  margin-top: 40rpx;
}
.header-fix {
  height: 128rpx;
  width: 750rpx;
  display: block;
}
.header-fix>.header {
  position: fixed;
  z-index: 1;
}
.header {
  height: 128rpx;
  width: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  padding-top: 40rpx;
  box-sizing: border-box;
  padding-top: 40rpx;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  background-color: #1a1a1a;
  position: relative;
}
.header-left,
.header-right {
  width: 88rpx;
  height: 88rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
}
.header-content {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
  font-size: 36rpx;
  text-align: center;
  font-size: 36rpx;
  color: #ffffff;
}
.btn {
  line-height: 96rpx;
  text-align: center;
  font-size: 36rpx;
  color: #ffffff;
  width: 570rpx;
  height: 96rpx;
  background-image: -webkit-linear-gradient(left,
		#deae42 0%,
		#c9912f 100%);
  background-image: linear-gradient(90deg,
		#deae42 0%,
		#c9912f 100%);
  border-radius: 10rpx;
  margin: 0 auto;
}
.mbtn {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  border-radius: 10rpx;
}