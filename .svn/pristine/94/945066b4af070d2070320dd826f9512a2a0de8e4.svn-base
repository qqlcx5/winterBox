
.msg-box {
  min-height: 100rpx;
  width: 750rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
  flex-direction: column;
  margin-top: 140rpx;
}
.msg-img {
  width: 200rpx;
  height: 200rpx;
  margin-bottom: 40rpx;
}
.msg-title {
  font-size: 28rpx;
  color: #999999;
  margin-bottom: 10rpx;
}
.msg-desc {
  color: #333333;
}