
.page {
  min-height: 100vh;
  width: 100%;
  background-color: #F7F7F7;
}
.header-box {
  height: 420rpx;
  width: 750rpx;
  background: url('http://pi23ib6iq.bkt.clouddn.com/banner1.png') no-repeat center;
  background-size: cover;
}
.Gheader {
  height: 128rpx;
  width: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  padding-top: 40rpx;
  box-sizing: border-box;
  padding-top: 40rpx;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  position: relative;
}
.msg-box {
  -webkit-flex-wrap: nowrap;
  flex-wrap: nowrap;
  margin: -40rpx auto 30rpx;
  width: 690rpx;
  height: 80rpx;
  background-color: #ffffff;
  box-shadow: 0 0 8rpx 0 #e8e8e8;
  border-radius: 10rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  font-size: 28rpx;
  color: #666666;
}
.msg-icon {
  font-size: 48rpx;
  color: #deae42;
  font-family: iconfont;
}
.msg-box swiper {
  width: 100%;
  height: 80rpx;
}
.msg-box swiper-item {
  line-height: 80rpx;
}
.list-box {
  min-height: 100rpx;
  width: 750rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
}
.item {
  width: 690rpx;
  min-height: 600rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  box-shadow: 0 0 8rpx 0 #d9d9d9;
  border-radius: 10rpx;
  background-color: #fff;
  overflow: hidden;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
  flex-direction: column;
  color: #ffffff;
  margin-bottom: 30rpx;
}
.item-img {
  width: 690rpx;
  height: 300rpx;
  background: url('http://pi23ib6iq.bkt.clouddn.com/banner4.png') no-repeat center;
  background-size: cover;
  position: relative;
}
.item-img-name {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  height: 60rpx;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  margin: 40rpx 0 10rpx;
}
.item-img-name>image {
  width: 60rpx;
  height: 60rpx;
  margin: 0 20rpx 0 40rpx;
}
.item-img-desc {
  font-size: 50rpx;
  margin-left: 120rpx;
}
.item-img-btn {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  height: 64rpx;
  width: 232rpx;
  border-radius: 32rpx;
  box-sizing: border-box;
  border: solid 2rpx #ffffff;
  align-items: center;
  margin: 40rpx 0 0 120rpx;
}
.item-img-btn image {
  width: 40rpx;
  height: 40rpx;
  margin-right: 10rpx;
}
.item-icon-box {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
  flex-wrap: wrap;
}
.item-icon {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
  flex-direction: column;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  color: #666666;
  width: 210rpx;
  height: 180rpx;
  background-color: #ffffff;
  border-radius: 10rpx;
  margin-top: 20rpx;
}
.item-icon>image {
  width: 60rpx;
  height: 60rpx;
  margin-bottom: 20rpx;
}
.item-icon-box .item-icon:nth-of-type(2),
.item-icon-box .item-icon:nth-of-type(5) {
  margin: 20rpx 30rpx 0;
}