
.page {
  width: 750rpx;
  height: 100vh;
  background-color: #F7F7F7;
}
.header-right text {
  position: absolute;
  right: 30rpx;
  top: 70rpx;
  font-size: 28rpx;
  color: #67ba61;
}
.list {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
  flex-direction: column;
  padding: 30rpx;
  box-sizing: border-box;
}
.item {
  height: 88rpx;
  line-height: 88rpx;
  width: 690rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  background-color: #FFFFFF;
  margin-bottom: 30rpx;
  border-radius: 10rpx;
}
.item-name {
  margin-left: 30rpx;
  width: 120rpx;
  color: #333333;
}
.item-shield {
  margin-left: 30rpx;
  width: 120rpx;
  color: #333333;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
}
.item-clear {
  width: 40rpx;
  height: 40rpx;
  margin-right: 30rpx;
}
.item-input {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
  height: 110rpx;
  line-height: 110rpx;
}